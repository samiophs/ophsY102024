import random
num = random.randint(1,10)
ask = int(input("enter a number 1-10"))
correct = False
while correct == False:
  ask = int(input("enter a number 1-10"))
if ask == num:
  print("guessed")
  correct = True
if ask > num:
  print("too low")
if ask < num:
  print("too high")
  
    
  
  
